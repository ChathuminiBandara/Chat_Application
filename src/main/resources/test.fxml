<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.image.Image?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="lk.ijse.TestController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <ScrollPane fx:id="scrollPane" layoutY="66.0" prefHeight="488.0" prefWidth="399.0" style="-fx-background-color: transparent;">
            <content>
                <VBox fx:id="vBox" prefHeight="486.0" prefWidth="397.0" />
            </content>
        </ScrollPane>
        <AnchorPane prefHeight="67.0" prefWidth="399.0" style="-fx-background-color: #095D54;">
            <children>
                <Label fx:id="lblClientName" layoutX="108.0" layoutY="20.0" prefHeight="23.0" prefWidth="118.0" text="WhatsApp" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="22.0" />
                    </font>
                </Label>
                <ImageView fitHeight="47.0" fitWidth="50.0" layoutX="48.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/user.png" />
                    </image>
                </ImageView>
                <ImageView fitHeight="33.0" fitWidth="40.0" layoutX="260.0" layoutY="18.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/video-call-1.png" />
                    </image>
                </ImageView>
                <ImageView fitHeight="28.0" fitWidth="33.0" layoutX="316.0" layoutY="21.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/632313.png" />
                    </image>
                </ImageView>
                <ImageView fitHeight="33.0" fitWidth="33.0" layoutX="360.0" layoutY="17.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/7066144.png" />
                    </image>
                </ImageView>
                <ImageView fitHeight="43.0" fitWidth="33.0" layoutX="4.0" layoutY="17.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/Arrow.png" />
                    </image>
                </ImageView>
            </children>
        </AnchorPane>
        <Button layoutX="334.0" layoutY="582.0" mnemonicParsing="false" onAction="#OnActionClientSend" prefHeight="16.0" prefWidth="0.0" textFill="WHITE">
            <graphic>
                <ImageView fitHeight="33.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/send.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <AnchorPane layoutX="14.0" layoutY="577.0" prefHeight="43.0" prefWidth="311.0" style="-fx-background-color: white;" styleClass="txtClient" stylesheets="@../CSS/CSS.css">
            <children>
                <TextField fx:id="txtClient" layoutX="39.0" onAction="#txtClientOnAction" prefHeight="43.0" prefWidth="233.0" promptText="Type a message" style="-fx-background-color: white;" />
                <ImageView fitHeight="33.0" fitWidth="28.0" layoutX="10.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/emoji.png" />
                    </image>
                </ImageView>
                <ImageView fitHeight="33.0" fitWidth="28.0" layoutX="272.0" layoutY="9.0" onMouseClicked="#ImageOnMouseClicked" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/icons8-attach-100.png" />
                    </image>
                </ImageView>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
